GRANT CREATE SESSION TO USUARIO1; --- DAR CONNECT
GRANT CREATE TABLE TO USUARIO1;---- DAR CREACION DE TABBLAS
GRANT CREATE SEQUENCE TO USUARI1;---- DAR CREACION DE SEQUENCIAS

REVOKE CREATE SESSION FROM USUARIO1--- PARA QUITAR 


WHERE ROUND(MONTHS_BETWEEN(SYSDATE, fecha_nacimiento)/12) >=70 -- SACAR AÃ‘OS


-------------------------------------------------------------------------------------
--1. Crear el ROL
CREATE ROLE rol_desarrollador;

--2. Asignar privilegios al ROL
GRANT CREATE SESSION TO rol_desarrollador;
GRANT CREATE TABLE TO rol_desarrollador;
GRANT CREATE SEQUENCE TO rol_desarrollador;
GRANT CREATE SYNONYM TO rol_desarrollador;
GRANT SELECT ON USUARIO1.CLIENTE to rol_desarrollador;
GRANT DELETE ON USUARIO1.CLIENTE to rol_desarrollador;

-- 3. Aisgnar el ROL al usuario

GRANT rol_desarrollador TO USUARIO1,USUARIO2;

-----------------------------------------
CREATE,DROP,ALTER,EXECUTE,TRUNCATE PRIVILEGIOS DE SISTEMA.
-----------------------------------------
SELECT,UPDATE,DELETE,INSERT,READ PRIVILEGIOS DE OBJETO 


