GRANT CREATE VIEW TO SOUND;
GRANT CREATE SYNONYM TO SOUND;
GRANT CREATE PUBLIC SYNONYM TO SOUND;
GRANT DROP PUBLIC SYNONYM TO SOUND;



CREATE VIEW vw_cliente as 
SELECT * FROM cliente ---- CREAR VISTA


CREATE SEQUENCE seq_cleinte; -- CREAR SEQUENCE

ALTER SEQUENCE seq_cleinte -- ALTERAR
INCREMENT BY 100;

CREATE SYNONYM syn_ciente for cliete; SINONIMO

CREATE PUBLIC SYNONYM synpub_ciente for cliete; -- SINONIMO PUBLICO

CREATE SYNONYM BILBAO_BKN_cliente for BILBAO_BKN.cliente; -- SINONIMO PRIVADO

DROP SEQUENCE seq_cliente - BORRAR

DROP SYNONYM syn_cliente -- BORRAR
